   ],
   "source": [
    "num = int(input(\"Enter an integer: \"))\n",
    "\n",
    "if num == 0 or num == 1:\n",
    "    print(\"Not Prime\")\n",
    "else:\n",
    "    for i in range(2, num):\n",
    "        if num % i == 0:\n",
    "            print(\"Not Prime\")\n",
    "            break\n",
    "    else:\n",
    "        print(\"Prime\")"
   ]
